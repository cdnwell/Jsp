<%
	ArrayList<HashSet<Integer>> list = new ArrayList<>();
	list = (ArrayList<HashSet<Integer>>)request.getAttribute("list");

	for(int i=0;i<list.size();i++){
		%>
		<%=i+1 %>번째 로또 번호<br>
		<table><tr>
		<%
		HashSet<Integer> set = new HashSet<>();
		set = list.get(i);
		for(int j : set){
			%>
			<td><%=j %></td>
			<%
		}
		%>
		</tr></table>
		<%
	}
%>
