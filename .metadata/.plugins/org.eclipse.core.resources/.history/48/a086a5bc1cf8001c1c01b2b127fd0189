<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
<%
	String[] lottos = (String[])request.getAttribute("lottos");

	for(int i=0;i<lottos.length;i++){
		String[] lotto = lottos[i].split(" ");
		%>
		<%=i+1 %>번째 로또 번호<br>
		<table><tr>
		<%
		for(int j=0;j<lotto.length;j++){
			%>
			<td><%=lotto[j] %></td>
			<%
		}
		%>
		</tr></table>
		<%
	}
%>
</body>
</html>