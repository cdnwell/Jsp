<%@page import="org.json.JSONObject"%>
<%@page import="java.util.Calendar"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<% 
	Calendar today = Calendar.getInstance();
	JSONObject obj = new JSONObject();
	//year,month,date,hour(hour_of_day),minute,second
	obj.put("year", Calendar.YEAR);
	obj.put("month", Calendar.MONTH);
	obj.put("data", Calendar.DATE);
	obj.put("hour", Calendar.HOUR_OF_DAY);
	obj.put("minute", Calendar.MINUTE);
	obj.put("second", Calendar.SECOND);
	
	out.write(obj.toString());
%>